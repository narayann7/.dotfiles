{
  "C_Cpp.updateChannel": "Insiders",
  "code-runner.saveFileBeforeRun": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "workbench.iconTheme": "office-material-icon-theme",
  "editor.suggestSelection": "first",
  "editor.mouseWheelZoom": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "dart.openDevTools": "flutter",
  "dart.debugSdkLibraries": false,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "editor.cursorBlinking": "expand",
  "editor.selectionHighlight": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.tabCompletion": "onlySnippets",
  "editor.wordBasedSuggestions": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.letterSpacing": 0.5,
  "editor.tabSize": 3,
  "editor.cursorStyle": "line",
  "editor.fontWeight": "400",
  "editor.autoIndent": "full",
  "editor.lineHeight": 26,
  "editor.fontSize": 15,
  "dart.previewFlutterUiGuidesCustomTracking": false,
  "markdown.preview.lineHeight": 2,
  "cSpell.userWords": [],
  "explorer.confirmDragAndDrop": false,

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.autoClosingBrackets": "always",
    "editor.tabSize": 1
  },

  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },

  "editor.quickSuggestionsDelay": 1,
  "editor.autoClosingQuotes": "always",
  "editor.autoClosingBrackets": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    // "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },

  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "workbench.startupEditor": "none",
  "terminal.integrated.gpuAcceleration": "off",
  "workbench.colorTheme": "VSLook",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": true,
    "go": false,
    "javascript": true,
    "html": true,
    "cpp": true
  },
  "editor.inlineSuggest.enabled": true,
  "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
  "bracketPairColorizer.depreciation-notice": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 6
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  //auto use fixes
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-ui-snippets.showNotesOnStartup": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.productIconTheme": "material-product-icons",
  "editor.accessibilitySupport": "off",
  "explorer.autoReveal": true,
  "[ahk]": {
    "editor.defaultFormatter": "mark-wiemer.vscode-autohotkey-plus-plus"
  },

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.toolsManagement.autoUpdate": true,
  "vslook.palette.type": "Material",
  "settingsSync.ignoredSettings": [],
  "vslook.palette.colors": {
    "base": {
      "1": "#101315"
    }
  },
  "dart.debugExternalPackageLibraries": false,
  "[go]": {
    "editor.defaultFormatter": "golang.go",

    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "editor.formatOnSave": true,
  "go.autocompleteUnimportedPackages": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
  "go.formatTool": "default",
  "vsicons.dontShowNewVersionMessage": true,
  "excalidraw.theme": "dark",
  "workbench.editorAssociations": {
    "*.edraw": "editor.excalidraw",
    "*.excalidraw": "editor.excalidraw"
  },
  "DockerRun.DisableAutoGenerateConfig": true,
  "editor.inlayHints.fontFamily": "Sen",
  "terminal.integrated.fontFamily": "Cascadia Mono",
  "terminal.integrated.fontWeight": "normal",
  "editor.codeLensFontFamily": "Sen",
  "scm.inputFontFamily": "Sen",
  "debug.console.fontFamily": "Sen",
  "notebook.outputFontFamily": "Sen",
  "errorLens.fontFamily": "Sen",
  "errorLens.fontSize": "Sen",
  "dart.devToolsBrowser": "default",
  "dart.customDevTools": {},
  "vscode-office.openOutline": false,
  "files.associations": {
    "*.cpp": "cpp"
  },

  //todo tree
  "todo-tree.general.tags": [
    "BUG",
    "bug",
    "HACK",
    "FIX",
    "fix",
    "TODO",
    "todo",
    "test",
    "TEST",
    "NOTE",
    "note",
    "DONE",
    "done",
    "REVIEW",
    "review",
    "remove",
    "REMOVE"
  ],

  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "type": "line",
      "foreground": "#ff8c00"
    },
    "todo": {
      "icon": "check",
      "type": "line",
      "foreground": "#ff8c00"
    },

    "FIX": {
      "foreground": "red",
      "icon": "alert",
      "type": "line"
    },
    "fix": {
      "foreground": "red",
      "icon": "alert",
      "type": "line"
    },
    "TEST": {
      "foreground": "skyblue",
      "icon": "check",
      "type": "line"
    },
    "test": {
      "foreground": "skyblue",
      "icon": "check",
      "type": "line"
    },
    "NOTE": {
      "foreground": "lightgreen",
      "icon": "note",
      "type": "line"
    },
    "note": {
      "foreground": "lightgreen",
      "icon": "note",
      "type": "line"
    },
    "DONE": {
      "foreground": "lightgreen",
      "icon": "check",
      "type": "line"
    },
    "done": {
      "foreground": "lightgreen",
      "icon": "check",
      "type": "line"
    },
    "REVIEW": {
      "foreground": "green",
      "icon": "eye",
      "type": "line"
    },
    "review": {
      "foreground": "green",
      "icon": "eye",
      "type": "line"
    },
    "REMOVE": {
      "foreground": "red",
      "icon": "alert",
      "type": "line"
    },
    "remove": {
      "foreground": "red",
      "icon": "alert",
      "type": "line"
    }
  }
}
