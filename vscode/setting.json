{
  "code-runner.saveFileBeforeRun": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.suggestSelection": "first",
  "editor.mouseWheelZoom": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "dart.openDevTools": "flutter",
  "dart.debugSdkLibraries": false,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "editor.cursorBlinking": "expand",
  "editor.selectionHighlight": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.tabCompletion": "onlySnippets",
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.letterSpacing": 0.5,
  "editor.tabSize": 3,
  "editor.cursorStyle": "line",
  "editor.fontWeight": "400",
  "editor.autoIndent": "full",
  "editor.lineHeight": 26,
  "editor.fontSize": 15,
  "dart.previewFlutterUiGuidesCustomTracking": false,
  "markdown.preview.lineHeight": 2,
  "cSpell.userWords": ["narayan", "Nymble", "Pallete"],
  "explorer.confirmDragAndDrop": false,
  "editor.maxTokenizationLineLength": 100000,
  //log line more than 1000
  "terminal.integrated.scrollback": 100000000,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.autoClosingBrackets": "always",
    "editor.tabSize": 1
  },

  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },

  "editor.quickSuggestionsDelay": 1,
  "editor.autoClosingQuotes": "always",
  "editor.autoClosingBrackets": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    // "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },

  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "workbench.startupEditor": "none",
  "terminal.integrated.gpuAcceleration": "off",
  "workbench.colorTheme": "VSLook",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": false,
    "go": false,
    "javascript": true,
    "html": true,
    "cpp": true,
    "sql": false
  },
  "editor.inlineSuggest.enabled": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 6
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  //auto use fixes
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-ui-snippets.showNotesOnStartup": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.productIconTheme": "material-product-icons",
  "editor.accessibilitySupport": "off",
  "explorer.autoReveal": true,
  "[ahk]": {
    "editor.defaultFormatter": "mark-wiemer.vscode-autohotkey-plus-plus"
  },

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.toolsManagement.autoUpdate": true,
  "vslook.palette.type": "Material",
  "settingsSync.ignoredSettings": [],
  "vslook.palette.colors": {
    "base": {
      "1": "#101315"
    }
  },
  "dart.debugExternalPackageLibraries": false,
  "[go]": {
    "editor.defaultFormatter": "golang.go",

    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "editor.formatOnSave": true,
  "go.autocompleteUnimportedPackages": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
  "go.formatTool": "default",
  "vsicons.dontShowNewVersionMessage": true,
  "excalidraw.theme": "dark",
  "workbench.editorAssociations": {
    "*.edraw": "editor.excalidraw",
    "*.excalidraw": "editor.excalidraw"
  },
  "DockerRun.DisableAutoGenerateConfig": true,
  "editor.inlayHints.fontFamily": "Sen",
  "terminal.integrated.fontWeight": "normal",
  "editor.codeLensFontFamily": "Sen",
  "scm.inputFontFamily": "Sen",
  "debug.console.fontFamily": "Sen",
  "errorLens.fontFamily": "Sen",
  "errorLens.fontSize": "Sen",
  "dart.devToolsBrowser": "default",
  "vscode-office.openOutline": false,
  "files.associations": {
    "*.cpp": "cpp",
    ".structure": "yaml"
  },

  "dart.showInspectorNotificationsForWidgetErrors": false,
  "dart.cliConsole": "terminal",
  "debug.console.lineHeight": 20,
  "intellicodeApiExamples.loggingLevel": "ALL",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "workbench.colorCustomizations": {},
  "npm.keybindingsChangedWarningShown": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "workbench.colorCustomizations": {},
  "workbench.colorTheme": "VSLook",
  "workbench.iconTheme": "office-material-icon-theme",
  "diffEditor.ignoreTrimWhitespace": false,
  "gitlens.views.commitDetails.files.layout": "list",
  "todo-tree.general.tags": ["todo", "fix", "bug", "review", "test"],
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.highlights.backgroundColourScheme": [],
  "todo-tree.highlights.foregroundColourScheme": ["orange"]
}
